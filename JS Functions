// File for storing different functions and examples
// Creating a function
function foo() {
}

// Self invoking function
(function () {
  var abc = "Hello!!";  // I will invoke myself
  alert( abc );  
})();

// Calling function when not self-invoked
foo();


// Nested functions
// Variables from the outer function can be used within the inner function

function outer() { 
  var x = 3;
  var y = 9;
  var z = y / x;
  function inner() {
    var v = 7;
    alert(typeOf(v*z));
  }
}


// Function adding a string and an integer
function sum(a, b) {
      return a + b;
    }
    sum(7, "4"); // Outputs "74"
// JS coverts the 7 into a string


